<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootbase.repository.AuthMapper">
    <select id="findByName" resultType="com.example.springbootbase.entity.AuthEntity">
        SELECT * FROM authorities WHERE user_name = #{userName};
    </select>
</mapper>